<!-- 图17.14: TransitionAnimations.fxml -->
<!-- 定义了一个Pane和一个Rectangle的FXML -->

<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Rectangle?>

<Pane prefHeight="150.0" prefWidth="150.0" 
   stylesheets="@CSSTransition.css" 
   xmlns="http://javafx.com/javafx/24" 
   xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Rectangle fx:id="rectangle" height="90.0" 
	     layoutX="30.0" layoutY="30.0" width="90.0" />
   </children>
</Pane>

